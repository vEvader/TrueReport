<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@Resources.MainResource.TrueReportTitle</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
    <body id="body">
        <div>
            @RenderBody()
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @Scripts.Render("~/bundles/knockout")
        @Scripts.Render("~/bundles/spin")
        @Scripts.Render("~/bundles/interact")
        @Scripts.Render("~/bundles/sweetalert")
        
        <script type="text/javascript" src="~/Scripts/app/tr.js"></script>
        <script type="text/javascript" src="~/Scripts/app/tr.ajaxProxy.js"></script>
        @RenderSection("scripts", required: false)
        @{ Html.RenderPartial("_Resources"); }
    </body>
</html>

